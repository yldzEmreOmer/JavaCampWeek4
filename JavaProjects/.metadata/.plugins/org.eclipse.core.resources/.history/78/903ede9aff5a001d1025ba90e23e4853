package kodlama.io.devs.entities.concretes;

import org.hibernate.validator.constraints.UniqueElements;

public class ProgrammerLanguage {

	@UniqueElements
	private int id;
	@UniqueElements
	private String name;

	public ProgrammerLanguage() {
		super();
	}

	public ProgrammerLanguage(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
